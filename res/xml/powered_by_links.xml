<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    
    <PreferenceCategory
  		android:title="@string/pc_partners">
  		<Preference
    		android:title="Weinstrassen"
    		android:summary="Visit website">
    		<intent 
        		android:action="android.intent.action.VIEW"
        		android:data="http://www.weinstrassen.at/"
    		/>
    	</Preference>
	</PreferenceCategory>
  	<PreferenceCategory
  		android:title="@string/pc_used_frameworks">
  		<Preference
    		android:title="ORMLite"
    		android:summary="Visit project's website">
    		<intent 
        		android:action="android.intent.action.VIEW"
        		android:data="http://ormlite.com/"
    		/>
		</Preference>
  		<Preference
    		android:title="SlidingMenu"
    		android:summary="Visit project on github">
    		<intent 
        		android:action="android.intent.action.VIEW"
        		android:data="https://github.com/jfeinstein10/SlidingMenu"
    		/>
		</Preference>
  		<Preference
    		android:title="ActionBarSherlock"
    		android:summary="Visit project on github">
    		<intent 
        		android:action="android.intent.action.VIEW"
        		android:data="https://github.com/JakeWharton/ActionBarSherlock"
    		/>
		</Preference>
  	</PreferenceCategory>
</PreferenceScreen>